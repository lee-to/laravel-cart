{
    "name": "lee-to/laravel-cart",
    "description": "Laravel cart module",
    "keywords": ["laravel", "cart"],
    "type": "library",
    "homepage": "https://github.com/lee-to/laravel-cart",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "leetodev@ya.ru",
            "homepage": "https://github.com/lee-to"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Leeto\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leeto\\Cart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leeto\\Cart\\Providers\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Leeto\\Cart\\Cart"
            }
        }
    }
}
